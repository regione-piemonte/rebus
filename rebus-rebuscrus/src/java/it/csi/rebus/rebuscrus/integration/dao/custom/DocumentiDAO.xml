<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper
	namespace="it.csi.rebus.rebuscrus.integration.dao.custom.DocumentiDAO">


	<select id="elencoDocumentiDesc" parameterType="java.lang.String"
		resultType="java.lang.String">

		SELECT rebus_d_tipo_documento.descrizione
		FROM rebus_d_tipo_documento
		WHERE id_tipo_documento = #{idTipoDocumento}

	</select>

</mapper>
